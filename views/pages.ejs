<div>
  <% if(page.count > 1){ %>
    <% if(page.number - 1){ %>
      <a href="/<%= page.number - 1 %>">上一页</a>
    <% } %>
    <% if(page.number + 1 < page.count){ %>
      <% if(page.number){ %>
        <span>&nbsp;&nbsp;</span>
      <% } %>
      <a href="/<%= page.number + 1 %>">下一页</a>
    <% } %>
  <% } %>
</div>
